html
  head
    title= title
    script.
      var wowhead_tooltips = { "colorlinks": true, "iconizelinks": true, "renamelinks": true };
      console.log("Whee");
    script( src='https://code.jquery.com/jquery-3.1.1.min.js' )
    script( src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css' )  
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js')
    script( src='http://wow.zamimg.com/widgets/power.js')

  body.jumbotron
      h1= title
      h1= message
      div
        table.table
          thead
            tr 
              th name
              th item
              th date
              th Bonus Lists
              th context
              th type
          tbody.table-striped
            each log in news
              tr
                td
                  p=log.characterName
                td
                  a(href="http://www.wowhead.com/item=" + log.itemId, rel="bonus=" + log.bonusListsFormattedWowhead ).q3=log.itemId 
                td
                  p=log.friendlyDate
                td
                  p=log.bonusLists
                td
                  p=log.context
                td
                  p=log.type